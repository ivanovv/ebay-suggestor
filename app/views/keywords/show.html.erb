<p id="notice"><%= notice %></p>


<%= @keyword.suggestion_count %> suggestions found.

<p>
  <strong>Value:</strong>
  <%= @keyword.value %>
</p>

<ul>
  <% @keyword.suggestions.each do |sug|%>
      <li><%= sug.value %></li>
      <ul>
        <% sug.variants.each do |v| %>
            <li><%=v%></li>
        <% end %>
      </ul>
  <% end %>
</ul>
